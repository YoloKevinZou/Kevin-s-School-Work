#include <iostream>
using namespace std;

int main(){
  int array[row][col];
  cout << "Enter the amount of row: ";
  cin >> row;
  cout << "Enter the amount of column: ";
  cin >> column;
  for(int r=0;r<row;r++)
    {
      for(int c=0;c<col;c++)
	{
	  cout << array[row][col];
	}
    }
  return 0;
}
