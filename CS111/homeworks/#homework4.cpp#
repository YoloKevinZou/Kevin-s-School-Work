#include <iostream>
using namespace std;

int main()
{
  int year; // in a variable as integer

  cout << "Enter a year "; // ask the user to enter a year
  
  cin >> year; //
  if (year%400==0)
    cout << "It is a leap year"<<endl;
  else if(year%100==0)
    cout << "It is NOT a leap year"<<endl;
  else if(year%4==0)
    cout<< "It is a leap year."<<endl;
  else 
    cout << "It is Not a leap year"<<endl;
return 0;
}
