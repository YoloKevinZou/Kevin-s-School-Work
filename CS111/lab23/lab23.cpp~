#include <iostream>
using namespace std;

int getGrades(int grades[], size)
{
  for ( int i = 0; i < size; i++)
    {
      cout << "Enter grade for: " << grades[i];
      cin >> grades[i];
    }
}

void printGrades(int grades[],size)
{
  for ( int i = 0; i < size; i++)
    {
      cout << grades[i] << " " << endl;
    }
}
 
int main(){
  int size;
  cout << "Enter the number of grades: ";
  cin >> size;
  int grades[size];
