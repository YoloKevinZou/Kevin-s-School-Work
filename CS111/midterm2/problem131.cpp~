#include <iostream>
using namespace std;

double plusTax(double cost, double taxRate)
{
  double plusTax=cost+taxRate;
  return plusTax;
}

int main() {
  int cost = 100; // cost is 100 cents
  double taxRate = 4.8; // tax is at 4.8 percent
  cout << "With tax that is " << plusTax(cost, taxRate) << " cents." << endl;
  return 0;
}
