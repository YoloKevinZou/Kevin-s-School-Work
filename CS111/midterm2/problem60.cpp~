#include <iostream>
using namespace std;

int main(){
  int a,b,sumMin=0;
  cout << "Enter a positive integer: ";
  cin >> a;
  cout << "Enter another positive integer: ";
  cin >> b;
  int array[a][b];
  for(int r = 0; r < a; r++)
    {
      for ( int c = 0; c < b; c++)
	{
	  cout << "Enter the value for " << r << " " << c;
	  cin >> array[r][c];
	}
    } 

  return 0;
