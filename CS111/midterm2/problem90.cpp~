#include <iostream>
using namespace std;

int main(){
  int n;srand(time(0));
  cout << "Enter a positive integer: ";
  cin >> n;
  int array[100];
  for( int i = 0; i < n; i++)
    {
      array[i]=rand()%30+1;
    }
  
  for ( int i = 0;i < n; i++)
    {
      cout << array[i];
    }
 

 return 0;
}
